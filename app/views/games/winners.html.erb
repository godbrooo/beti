<%= simple_form_for @game, url: winners_path, method: :patch do |f| %>
   <% @game.bets.each do |bet| %>
 <%= f.simple_fields_for :prizes do |pr| %>
     <%= bet.user.email %>
     <%= pr.association :user, as: :hidden, input_html: {value: bet.user} %>
     <%= pr.input :ranking, collection: @ranking_possibilities %>
   <% end %>
 <% end %>
 <%= f.submit "Fermer le pari et lancer les paiments" %>
<% end %>















